{
  "name": "@foo-i18n/plurals",
  "version": "1.0.0",
  "description": "Plural rules for message translation.",
  "author": "Yanick Rochon <yanick.rochon@gmail.com>",
  "keywords": [
    "i18n",
    "translator",
    "translate",
    "intl",
    "locale",
    "plural"
  ],
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    "./*": "./build/locale/*",
    "./types": "./build/types.js"
  },
  "scripts": {
    "build": "pnpm clean && pnpm generate && tsc -p tsconfig.build.json && ts-node ../../scripts/bundle.ts",
    "generate": "ts-node generate.ts",
    "clean": "rm -rf build coverage",
    "lint": "eslint . --max-warnings 0",
    "test": "jest",
    "reset": "rimraf .next .turbo node_modules"
  },
  "dependencies": {
    "@foo-i18n/base": "workspace:*"
  },
  "devDependencies": {
    "@foo-i18n/eslint-config": "workspace:*",
    "@foo-i18n/typescript-config": "workspace:*",
    "@jest/types": "29.6.3",
    "@turbo/gen": "^1.12.4",
    "@types/eslint": "^8.56.5",
    "@types/jest" : "^29.5.12",
    "@types/node": "^20.11.24",
    "cldr-data": "^36.0.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
